package Section2;

import java.util.Scanner;

public class Code18 {

	public static void main(String[] args) {
		 Scanner keyboard = new Scanner(System.in);
		 int a = keyboard.nextInt();
		 int [] data = new int [a];
		 for (int i = 0; i < a; i++)
			 data[i] = keyboard.nextInt();
		 keyboard.close();
	}
}
