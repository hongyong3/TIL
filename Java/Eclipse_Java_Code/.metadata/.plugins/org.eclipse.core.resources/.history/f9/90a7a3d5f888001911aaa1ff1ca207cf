import java.util.Scanner;

public class Star3 {

	public static void main(String[] args) {
		Scanner keyboard = new Scanner(System.in);
		int a = keyboard.nextInt();
		keyboard.close();

		for (int i = a; i <= a; i++) {
			for (int j = a - i; j <= a; j++) {
				System.out.print("*");
			}
		}
		System.out.println();
	}
}
